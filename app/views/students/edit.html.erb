<h1>Student Form</h1>
<%= form_tag students_path, method: "put" do %>
<%= hidden_field_tag :id, @student.id %>
<label>First Name:</label> <%= text_field_tag :'student[first_name]', @student.first_name, id: 'student_first_name' %>
<label>Last Name:</label>  <%= text_field_tag :'student[last_name]', @student.last_name, id: 'student_last_name' %>
<%= submit_tag "Update Student" %>
<% end %>
